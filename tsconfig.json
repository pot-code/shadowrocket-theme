{
  "compilerOptions": {
    "baseUrl": "./",
    "jsx": "react",
    "sourceMap": true,
    "module": "es6",
    "moduleResolution": "node",
    "target": "es6",
    "typeRoots": [
      "./src/typings"
    ],
    "lib": [
      "dom",
      "es6"
    ],
    "paths": {
      "@components/*": [
        "src/components/*"
      ],
      "@stores/*": [
        "src/store/*"
      ]
    }
  },
  "include": [
    "src/**/*",
  ]
}
